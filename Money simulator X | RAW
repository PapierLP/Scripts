--Spawn Functions
spawn(function()
    while wait() do
        if _G.NoClip then
            local stepped
            stepped = game:GetService("RunService").Stepped:Connect(function()
                game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
            end)
            repeat wait() until not _G.NoClip
            stepped:Disconnect()
        end
    end
end)
