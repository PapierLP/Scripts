--Function
function craftItem(itemName,quantity)
    if not string.find(itemName, "Money") then
        local output = math.round(quantity / craftingRecipes[itemName].output+0.4)
        for i,v in pairs(craftingRecipes[itemName].craftingMaterial) do
            if player.Stats[i].Value < math.round(v*quantity / craftingRecipes[itemName].output+0.4) then
                local difference = math.round(v*quantity - player.Stats[i].Value / craftingRecipes[i].output+0.4)
                craftItem(i,difference)
                player.Stats[i].Changed:Wait()
                if player.Stats[i].Value <= math.round(v*quantity / craftingRecipes[itemName].output+0.4) then
                    craftItem(itemName,output)
                end
            end
        end
        game:GetService("ReplicatedStorage").Craft:FireServer(craftingRecipes[itemName].id,output)
    end
end

