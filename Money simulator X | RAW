spawn(function()
    while wait() do
        if _G.AutoKillMobs then
            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                if string.find(v.Name, "ComputerDefence") and v:FindFirstChild("Enemies") then
                    for i2,v2 in pairs(v.Enemies:GetChildren()) do
                        if v2:FindFirstChildOfClass("ClickDetector") then fireclickdetector(v2:FindFirstChildOfClass("ClickDetector")) end
                    end
                end
            end
        end
    end
end)
