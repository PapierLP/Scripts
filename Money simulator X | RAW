            game:GetService("ReplicatedStorage").UpgradeRank:FireServer()
        end
    end
end)
spawn(function()
    while wait() do
        if _G.DisableCoinRain then
            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                if string.find(v.Name, "CoinLevel") then
                    game:GetService("Debris"):AddItem(v, 0)
                end
            end
            local childadded
            childadded = game:GetService("Workspace").ChildAdded:Connect(function(child)
                if string.find(child.Name, "CoinLevel") then
                    game:GetService("Debris"):AddItem(child, 0)
                end
            end)
            repeat wait() until not _G.DisableCoinRain
            childadded:Disconnect()
        end
    end
end)
spawn(function()
    while wait() do
        if _G.AutoCollectGems then
            for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
                if string.find(v.Name, "Gem") and v:FindFirstChildOfClass("ClickDetector") and not string.find(v.Name, "Spawner") then
                    fireclickdetector(v:FindFirstChildOfClass("ClickDetector"))
                end
            end
        end
    end
end)
spawn(function()
    while wait() do
        if _G.AutoRefill then
            game:GetService("ReplicatedStorage").FillMoney:FireServer()
        end
    end
end)
spawn(function()
    while wait() do
        if _G.AutoClick then
            local stepped
            stepped = game:GetService("RunService").Stepped:Connect(function()
                game:GetService("ReplicatedStorage").GetMoney:FireServer("DropOn")
                if not _G.AutoClick then stepped:Disconnect() end
            end)
            repeat wait() until not _G.AutoClick
        end
    end
end)
spawn(function()
    while wait() do
        if _G.OreKillAura then
            if not game.Players.LocalPlayer.Character:FindFirstChild("Sphere") then
                local Sphere = Instance.new("Part")
                Sphere.Name = "Sphere"
                Sphere.Shape = "Ball"
                Sphere.Color = SphereColor
                Sphere.CastShadow = false
                Sphere.Material = "ForceField"
                Sphere.Size = Vector3.new(SphereSize*2,SphereSize*2,SphereSize*2)
                Sphere.CanCollide = false
                Sphere.CanTouch = false
                Sphere.Massless = true
                local Weld = Instance.new("WeldConstraint")
                Weld.Part0 = game.Players.LocalPlayer.Character.PrimaryPart
                Weld.Part1 = Sphere
                Sphere.Position = game.Players.LocalPlayer.Character.PrimaryPart.Position
                Weld.Parent = Sphere
                Sphere.Parent = game.Players.LocalPlayer.Character
            end
        else
            if game.Players.LocalPlayer.Character:FindFirstChild("Sphere") then
                game.Players.LocalPlayer.Character:FindFirstChild("Sphere"):Destroy()
            end
        end
        while _G.OreKillAura and wait() do
            for i,v in pairs(game:GetService("Workspace").Ores:GetChildren()) do
                if v.CanCollide == true then
                    if (game.Players.LocalPlayer.Character.PrimaryPart.Position - v.Position).Magnitude < SphereSize then
                        fireclickdetector(v:FindFirstChildOfClass("ClickDetector"))
                    end
                end
            end
        end
    end
end)

-- ANTI AFK
_G.Ai_Enled = true
while wait() do
    if game:IsLoaded() then
        break
    end
end
wait(1)
if _G.Ai_Enled then
    game.StarterGui:SetCore(
        "ChatMakeSystemMessage",
        {
            Text = "[Script] Loaded !",
            Font = Enum.Font.SourceSansBold,
            FontSize = Enum.FontSize.Size24
        }
    )

    while wait(15) do
        local VirtualUser = game:service "VirtualUser"
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end
end
